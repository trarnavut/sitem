<Window x:Class="HealthApp.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="BeHealthy Web" Height="400" Width="600">
    <Grid>
       
        <Frame Name="WebFrame"
               HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
               Margin="10,50,10,10" />

        
        <Button Content="Show Web Page"
                HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="10,10,0,0" Width="120"
                Click="ShowWebPageButton_Click" />
        <Button Content="Back to Menu"
                HorizontalAlignment="Right" VerticalAlignment="Top"
                Margin="0,10,10,0" Width="120"
                Click="BackToMenuButton_Click" />
    </Grid>
</Window>
